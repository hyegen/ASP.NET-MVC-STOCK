@using MvsStok.Models.Entity
@model List<TBLMUSTERILER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>MUSTERI ID</th>
        <th>MUSTERI AD</th>
        <th>MUSTERI SOYAD</th>
        <th>Sil</th>
        <th>Güncelle</th>
        
        
    </tr>
    <tbody>
        @foreach(var mstr in Model)
            {
                <tr>
                    <th>@mstr.MUSTERIID</th>
                    <td>@mstr.MUSTERIAD</td>
                    <td>@mstr.MUSTERISOYAD</td>
                    <td><a href="/MUSTERI/SIL/@mstr.MUSTERIID" class="btn btn-danger" >SİL</a></td>
                    <td><a href="/MUSTERI/GUNCELLE/@mstr.MUSTERIID" class="btn btn-success" >GÜNCELLE</a></td>
     
                </tr>
            }
    </tbody>
</table>

<a href="/MUSTERI/YeniMusteri/" class="btn btn-primary">YENİ MÜŞTERİ EKLE</a>
