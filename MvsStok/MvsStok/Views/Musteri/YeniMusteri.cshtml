
@{
    ViewBag.Title = "YeniMusteri";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<form method="post" class="form-group">
    <div>
        <label>Müşteri Adı </label>
        <input type="text" class="form-control" name="MUSTERIAD" placeholder="Müşteri Adı Giriniz" />
    </div>
    <div>
        <label>Müşteri Soyadı </label>
        <input type="text" class="form-control" name="MUSTERISOYAD" placeholder="Müşteri Soyadı Giriniz" />
    </div>
    <div style="margin-top:15px">
        <button class="btn btn-info">Müşteri Ekle</button>
    </div>
</form>